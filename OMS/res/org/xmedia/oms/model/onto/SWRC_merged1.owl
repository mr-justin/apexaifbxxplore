            <publication>
              <Publication rdf:about="http://www.aifb.uni-karlsruhe.de/Publikationen/viewPublikationOWL/id633instance">
                <author rdf:resource="http://www.aifb.uni-karlsruhe.de/Publikationen/viewExternerAutorOWL/id7instance"/>
