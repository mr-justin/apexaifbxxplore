                                                    <title rdf:datatype=
                                                    "http://www.w3.org/2001/XMLSchema#string"
                                                    >FRED: Ontology-based Agents for enabling E-Coaching Support in a large Company</title>
                                                    <year rdf:datatype=
                                                    "http://www.w3.org/2001/XMLSchema#string"
                                                    >2002</year>
                                                    <booktitle
                                                     rdf:datatype=
                                                    "http://www.w3.org/2001/XMLSchema#string"
                                                    >Second International Workshop on Ontologies in Agent Systems (OAS 2002) , held at the 1st International Conference on Autonomous Agents &amp; Multiagent Systems, 15 or 16 July 2002, Bologna, Italy.</booktitle>
                                                    <rdf:type rdf:resource="#Publication"/>
                                                    <author rdf:resource="http://www.aifb.uni-karlsruhe.de/Personen/viewPersonOWL/id20instance"/>
