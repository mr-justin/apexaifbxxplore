                                <title rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                                >OntoManager - a system for the usage-based ontology management</title>
                                <author rdf:resource="http://www.aifb.uni-karlsruhe.de/Personen/viewPersonOWL/id88instance"/>
                                <month rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                                >December</month>
                                <volume rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                                >2888</volume>
