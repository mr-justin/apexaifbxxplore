<owl:Class rdf:about="http://protege.stanford.edu/kb#BackTackle"
	 rdfs:label="BackTackle">
	<rdfs:comment>An attempt by a defender to take the ball away from a ball carrier by swinging the defender's leg in front of the ball from behind.</rdfs:comment>
	<rdfs:subClassOf rdf:resource="http://protege.stanford.edu/kb#Tackle"/>
</owl:Class>
