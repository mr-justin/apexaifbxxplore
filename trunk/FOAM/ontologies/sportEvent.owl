	<rdfs:subClassOf rdf:resource="http://protege.stanford.edu/kb#BallAction"/>
	<linginfo:lingInfo rdf:resource="http://protege.stanford.edu/kb#SUMO_linginfo_protege_football_18_Nov_Instance_2"/>
