  <swrc:pages rdf:datatype="http://www.w3.org/2001/XMLSchema#string">242-255</swrc:pages>
  <swrc:month rdf:datatype="http://www.w3.org/2001/XMLSchema#string">March</swrc:month>
 </rdf:Description>
