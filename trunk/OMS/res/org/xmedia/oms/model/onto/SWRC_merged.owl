  <swrc:author rdf:type="http://swrc.ontoware.org/ontology#Person"  rdf:resource="http://www.aifb.uni-karlsruhe.de/Personen/viewPersonOWL/id20instance"/>
  <swrc:author rdf:type="http://swrc.ontoware.org/ontology#Person"  rdf:resource="http://www.aifb.uni-karlsruhe.de/Publikationen/viewExternerAutorOWL/id633instance"/>
