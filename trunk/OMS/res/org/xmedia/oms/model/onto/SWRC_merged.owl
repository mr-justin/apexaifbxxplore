  <swrc:volume rdf:datatype="http://www.w3.org/2001/XMLSchema#string">10</swrc:volume>
  <swrc:number rdf:datatype="http://www.w3.org/2001/XMLSchema#string">4</swrc:number>
 </rdf:Description>
