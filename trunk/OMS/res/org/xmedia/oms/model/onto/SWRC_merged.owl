  <swrc:journal rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Artificial Intelligence, Elsevier</swrc:journal>
 </rdf:Description>
 <rdf:Description rdf:about="http://www.aifb.uni-karlsruhe.de/Publikationen/viewPublikationOWL/id474instance">
