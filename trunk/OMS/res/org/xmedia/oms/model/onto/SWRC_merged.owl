  <swrc:booktitle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Proc. Referenzmodellierung 2000, Siegen, Germany, October 12-13, 2000</swrc:booktitle>
  <swrc:month rdf:datatype="http://www.w3.org/2001/XMLSchema#string">October</swrc:month>
