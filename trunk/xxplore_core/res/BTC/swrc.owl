    <booktitle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Intelligent Exploration of the Web</booktitle>
    <title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Bootstrapping an Ontology-based Information Extraction System</title>
    <rdf:type rdf:resource="#Publication"/>
    <rdf:type rdf:resource="#InCollection"/>
