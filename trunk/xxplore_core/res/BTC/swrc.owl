    <series rdf:datatype="http://www.w3.org/2001/XMLSchema#string">LNI</series>
    <volume rdf:datatype="http://www.w3.org/2001/XMLSchema#string">51</volume>
    <rdf:type rdf:resource="#Publication"/>
    <author rdf:resource="http://www.aifb.uni-karlsruhe.de/Personen/viewPersonOWL/id2126instance"/>
