  <rdf:Description rdf:about="http://www.aifb.uni-karlsruhe.de/Publikationen/viewPublikationOWL/id471instance">
    <title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The Unified Problem-solving Method Development Language UPML</title>
    <author rdf:resource="http://www.aifb.uni-karlsruhe.de/Publikationen/viewExternerAutorOWL/id312instance"/>
    <rdf:type rdf:resource="#Article"/>
