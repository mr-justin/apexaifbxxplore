    <author rdf:resource="http://www.aifb.uni-karlsruhe.de/Personen/viewPersonOWL/id50instance"/>
    <title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">AI for the Web - Ontology-based Community Web Portals</title>
    <author rdf:resource="http://www.aifb.uni-karlsruhe.de/Personen/viewPersonOWL/id20instance"/>
