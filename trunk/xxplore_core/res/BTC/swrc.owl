    <title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">m-Advertising: Werbung mit mobilen Endgeräten - ein Überblick</title>
    <rdf:type rdf:resource="#InProceedings"/>
    <pages rdf:datatype="http://www.w3.org/2001/XMLSchema#string">103-114</pages>
