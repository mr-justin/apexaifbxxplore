  <rdf:Description rdf:about="http://www.aifb.uni-karlsruhe.de/Publikationen/viewPublikationOWL/id992instance">
    <author rdf:resource="http://www.aifb.uni-karlsruhe.de/Publikationen/viewExternerAutorOWL/id530instance"/>
