  <rdf:Description rdf:about="http://www.aifb.uni-karlsruhe.de/Publikationen/viewPublikationOWL/id1148instance">
    <rdf:type rdf:resource="#Publication"/>
    <rdf:type rdf:resource="#TechnicalReport"/>
    <author rdf:resource="http://www.aifb.uni-karlsruhe.de/Publikationen/viewExternerAutorOWL/id589instance"/>
