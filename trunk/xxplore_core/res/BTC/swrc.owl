  <rdf:Description rdf:about="http://www.aifb.uni-karlsruhe.de/Publikationen/viewPublikationOWL/id475instance">
    <year rdf:datatype="http://www.w3.org/2001/XMLSchema#string">2001</year>
    <journal rdf:datatype="http://www.w3.org/2001/XMLSchema#string">IEEE Intelligent Systems</journal>
