    <booktitle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">19th Workshop on (Constraint) Logic Programming, Ulm, Germany, February 2005</booktitle>
    <series rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ulmer Informatik-Berichte</series>
    <month rdf:datatype="http://www.w3.org/2001/XMLSchema#string">February</month>
