    <rdf:type rdf:resource="#Publication"/>
    <booktitle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Proc. of 13th European Conference on Knowledge Engineering and Knowledge Management (EKAW)</booktitle>
    <title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">MAFRA - A Mapping Framework for Distributed Ontologies</title>
