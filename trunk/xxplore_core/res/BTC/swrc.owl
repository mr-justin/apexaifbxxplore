    <booktitle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Proceedings of the Third International Conference on Practical Aspects of Knowledge Management, Basel, Switzerland, October 30-31, 2000</booktitle>
    <year rdf:datatype="http://www.w3.org/2001/XMLSchema#string">2000</year>
    <author rdf:resource="http://www.aifb.uni-karlsruhe.de/Personen/viewPersonOWL/id60instance"/>
